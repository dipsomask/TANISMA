<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/profileLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/foneprofilecolor"
    android:backgroundTint="@color/foneprofilecolor"
    tools:context=".ProfileActivity">

    <ImageButton
        android:id="@+id/homeBottoneInprofile"
        android:layout_width="0dp"
        android:layout_height="100dp"
        android:layout_marginEnd="305dp"
        android:layout_weight="1"
        android:contentDescription="@string/homebuttone"
        android:foreground="@drawable/cherry_icon"
        android:onClick="goHome"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/cherry_icon" />

    <TextView
        android:id="@+id/profileHeadname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:text="@string/profilebutton"
        android:textSize="23sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.458"
        app:layout_constraintStart_toEndOf="@+id/homeBottoneInprofile"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>